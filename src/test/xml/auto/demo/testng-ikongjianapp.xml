<?xml version="1.0" encoding="UTF-8"?>
<suite name="demo-app" parallel="false">
  <listeners>
  	<listener class-name="com.automation.listener.TestNGListener" />
    <listener class-name="com.automation.listener.RetryListener" />
    <listener class-name="com.automation.listener.IReporterListener" />
  </listeners>
  <parameter name ="handlerMode" value="Appium" />
  <parameter name ="testFor" value="contact" />
  <test name="app">
	<parameter name = "noReset" value="false" />
	<parameter name = "port" value="4723" />
	<parameter name = "udid" value="127.0.0.1:62001" /> 
	<parameter name = "platformVersion" value="7.1.0" />
	<parameter name = "appPackage" value="com.ikongjian.decoration" />
	<parameter name = "appActivity" value="com.ikongjian.decoration.activity.SplashActivity" />	
    <groups>
		<define name="all">
			<include name="auto.demo.test.ikongjianapp" />
		</define>
		<run>  
			<include name="all"/> 
		</run>  
	</groups>
    <classes>
    	<class name="auto.app.script.TestCaseOfIkongjian" />
    </classes>
  </test>
</suite>
